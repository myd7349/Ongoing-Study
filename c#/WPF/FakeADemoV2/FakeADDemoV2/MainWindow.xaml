<Window x:Class="FakeADDemoV2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        xmlns:local="clr-namespace:FakeADDemoV2"
        mc:Ignorable="d"
        Title="Fake AD Demo" Height="450" Width="800"
        Loaded="Window_Loaded"
        Closed="Window_Closed">
    <Grid>
        <d3:ChartPlotter x:Name="adDataChart_" Margin="10,10,10,10">
            <d3:LineGraph x:Name="adDataLineGraph_" />
        </d3:ChartPlotter>
    </Grid>
</Window>
